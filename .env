# مفتاح التشفير السري لمشروع Django
SECRET_KEY=django-insecure-استبدل_هذا_المفتاح_بمفتاح_حقيقي

# وضع التصحيح (False في الإنتاج)
DEBUG=True

# إعدادات قاعدة البيانات (مثال على PostgreSQL)
DB_NAME=your_db_name
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_HOST=localhost
DB_PORT=5432

# إعدادات Cloudinary
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# إعدادات البريد الإلكتروني (مثال على Gmail)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USE_TLS=True
EMAIL_HOST_USER=your_email@gmail.com
EMAIL_HOST_PASSWORD=your_email_password
